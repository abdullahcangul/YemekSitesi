@model List<Kategori>


    <ul class="boxed">
        @foreach (var kategori in @Model)
        {
            <li class="light"><a href="recipes.html" title="@kategori.kategoriAdi"><img src="/Content/Resimler/Kategoriler/@kategori.resim"/> <span>@kategori.kategoriAdi</span></a></li>

        }
    </ul>